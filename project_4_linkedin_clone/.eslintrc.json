{
	"extends": "next/core-web-vitals",
	"rules": {
		"semi": [
			"error",
			"always"
		],
		"comma-dangle": [
			"error",
			{
				"arrays": "always",
				"objects": "always",
				"imports": "always",
				"exports": "always"
			}
		],
		"camelcase": "error",
		"indent": [
			"error",
			"tab"
		]
	}
}